<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
       <record id="Requester_form" model="ir.ui.view">
        <field name="name">Requester_form</field>
        <field name="model">registration.request</field>
        <field name="arch" type="xml">
            <form string="Requester">
                <sheet>
                    <group>
                       <field name="Requester" />
                       <field name="Program_Name" />
                       <field name="Coach_Name" />
                    </group>
                        <group col="4">
                            <field name="Request_Date" />
                            <field name="Date_From" />
                            <field name="Date_To" />
                        </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="Requester_tree" model="ir.ui.view">
        <field name="name">Requester_tree</field>
        <field name="model">registration.request</field>
        <field name="arch" type="xml">
            <tree string="Requester">
                  <field name="Requester" />
                  <field name="Program_Name" />
                  <field name="Coach_Name" />
                  <field name="Request_Date" />
                  <field name="Date_From" />
                  <field name="Date_To" />
            </tree>
        </field>
    </record>

    <record id="Requester_search" model="ir.ui.view">
         <field name="name">Requester_Search</field>
         <field name="model">registration.request</field>
         <field name="arch" type="xml">
        <search>
          <field name ="Requester"/>
            <filter string="coach_ali" name="Coach_Name"
          domain="[('Coach_Name','in',['ali'])]"/>
          <filter string="coach_khalid" name="Coach_Name"
          domain="[('Coach_Name','in',['khalid'])]"/>
            <filter string="Program_Body_Building" name="Program_Name"
          domain="[('Program_Name','in',['Body_Building'])]"/>
          <filter string="Program_Weight_Loss" name="Program_Name"
          domain="[('Program_Name','in',['Weight_Loss'])]"/>
          <filter string="Request_Date" name="Request_Date" date="Request_Date"/>
        </search>

      </field>
    </record>


    <record id="menu_Requester_action" model="ir.actions.act_window">
        <field name="name">registration_menu</field>
         <field name="res_model">registration.request</field>
        <field name="view_mode">tree,form</field>
    </record>

